<h1>Users List</h1>

<ul>
    <% @users.each do |user| %>
        <li>
            <%= user.first_name %>
        </li>
        <li>
            <%= user.last_name %>
        </li>
        <li>
            <%= user.email %>
        </li>
    <% end %>
</ul>

<%= link_to "New User", register_path, class: "submit-btn" %>